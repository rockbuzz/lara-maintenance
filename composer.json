{
  "name": "rockbuzz/lara-maintenance",
  "description": "Middleware to manage maintenance mode with ips activation",
  "keywords": [],
  "license": "MIT",
  "authors": [
    {
      "name": "TiagoDevWeb",
      "email": "tiago@rockbuzz.com.br"
    },
    {
      "name": "Paulo Henrique P. Scheuermann",
      "email": "paulo@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraMaintenance\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "^3.7",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraMaintenance\\ServiceProvider"
      ]
    }
  }
}
